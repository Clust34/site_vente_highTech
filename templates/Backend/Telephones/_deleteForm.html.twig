<form action="{{ path('admin.telephones.delete') }}" method="POST" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet article Téléphone?')">
	<input type="hidden" name="id" value="{{ telephone.id }}">
	<input type="hidden" name="token" value="{{ csrf_token('delete' ~ telephone.id) }}">
	<button type="submit" class="btn btn-danger">Supprimer</button>
</form>
